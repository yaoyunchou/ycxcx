/**index.wxss**/
.container .obligate{
  box-sizing: border-box;
  width:100%;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  background-color:#f1efef;
  margin-bottom:28rpx;
  position: relative;
}

.container .article{
  font-size:38rpx;
  display: block;
  line-height: 70rpx;
  min-height: 150rpx;
  margin-bottom: 30rpx;
  color:#515567;
}

.container .head{
  height: 120rpx;
  line-height: 55rpx;
  position: relative;
}
.container .head .line{
  position: absolute;
  height: 1px;
  width:90%;
  border-top:1px solid #ef8200;
  top:55rpx;
  left:5%;
  z-index: -1;
}
.container .head .text{
  color:#ef8200;
  width:200rpx;
  margin:0 auto;
  text-align: center;
  font-size:38rpx;
  background: #fff;
}
.content{
  line-height: 70rpx;
  padding:0 10px;
}